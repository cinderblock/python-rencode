{
  "name": "python-rencode",
  "version": "1.2.0",
  "description": "Arbitrary data encoder/decoder that matches python rencode",
  "main": "rencode",
  "types": "rencode.d.ts",
  "scripts": {
    "repl": "ts-node repl",
    "format": "prettier --write \"**/*.@(js?(on)|md|prettierrc|clang-format|yml)\"",
    "ci": "yarn test && yarn coveralls",
    "test": "jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "preversion": "yarn format && yarn test",
    "postversion": "npm publish",
    "build": "tsc --declaration",
    "setup": "yarn && install-vscode-extensions",
    "prepublishOnly": "yarn build",
    "postpublish": "git push origin master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinderblock/python-rencode.git"
  },
  "keywords": [
    "bencode",
    "rencode",
    "python rencode",
    "pip rencode"
  ],
  "files": [
    "rencode.js",
    "rencode.ts",
    "rencode.d.ts"
  ],
  "author": "Cameron Tacklind <cameron@tacklind.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cinderblock/python-rencode/issues"
  },
  "homepage": "https://github.com/cinderblock/python-rencode#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "coveralls": "^3.0.2",
    "install-vscode-extensions": "^1.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6",
    "yarn": "^1.12.3"
  }
}
