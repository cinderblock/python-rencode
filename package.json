  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "babel-jest": "^23.6.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "yarn": "^1.12.3"
  }
