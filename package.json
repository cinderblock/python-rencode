{
  "name": "python-rencode",
  "version": "1.3.2",
  "description": "Arbitrary data encoder/decoder that matches python rencode",
  "main": "src/rencode",
  "types": "src/rencode.d.ts",
  "scripts": {
    "repl": "ts-node repl",
    "format": "prettier --write \"**/*.@(ts|js?(on)|md|prettierrc|clang-format|yml)\"",
    "ci": "yarn test --ci && yarn coveralls",
    "test": "jest",
    "clean": "rm src/rencode.js src/rencode.d.ts",
    "coveralls": "coveralls < coverage/lcov.info",
    "preversion": "yarn format && yarn test",
    "postversion": "npm publish",
    "build": "tsc --declaration",
    "setup": "yarn && install-vscode-extensions",
    "prepublishOnly": "yarn build",
    "postpublish": "git push origin master --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinderblock/python-rencode.git"
  },
  "keywords": [
    "bencode",
    "rencode",
    "python rencode",
    "pip rencode"
  ],
  "files": [
    "src/rencode.js",
    "src/rencode.ts",
    "src/rencode.d.ts"
  ],
  "author": "Cameron Tacklind <cameron@tacklind.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cinderblock/python-rencode/issues"
  },
  "homepage": "https://github.com/cinderblock/python-rencode#readme",
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.0",
    "coveralls": "^3.0.9",
    "install-vscode-extensions": "^1.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "prettier": "^1.19.1",
    "python-shell": "^1.0.8",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "yarn": "^1.21.1"
  }
}
